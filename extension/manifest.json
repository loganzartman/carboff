{
    "name": "carboff",
    "version": "1.0",
    "description": "stop leaving carb-on",
    "permissions": [
      "storage",
      "tabs",
      "webRequest",
      "debugger",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["index.js"],
      "persistent": true
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}
